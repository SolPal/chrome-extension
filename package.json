        "date-fns": "^2.30.0",
        "openai": "^4.12.1",
        "react-day-picker": "^8.8.2",
        "react-hook-form": "^7.47.0",
        "react-icons": "^4.11.0",
        "react-textarea-autosize": "^8.5.3",
