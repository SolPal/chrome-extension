{
    "name": "solpal-chrome-extension",
    "longName": "Solpal.ai",
    "description": "Personal web companion for everything web3 and Solana related",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "start": "concurrently \"npm run dev\" \"npm run build\"",
        "tailwind": "tailwindcss -i src/styles/index.css -o dist/assets/index.css",
        "dev": "vite",
        "build": "tsc && vite build && npm run build:js",
        "build:pre": "esno scripts/prebuild.ts",
        "build:post": "esno scripts/postbuild.ts",
        "build:js": "vite build --config vite.config.content.ts",
        "preview": "vite preview"
    },
    "author": "Rafael Gomes <eng.rsgomes@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/chrome": "^0.0.240",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.4.1",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/webextension-polyfill": "^0.10.1",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "esno": "^0.16.3",
        "fs-extra": "^11.1.1",
        "postcss": "^8.4.25",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "unplugin-auto-import": "^0.16.6",
        "vite": "^4.4.3",
        "vite-plugin-static-copy": "^0.16.0",
        "webext-bridge": "^6.0.1",
        "webextension-polyfill": "^0.10.0",
        "webextension-polyfill-ts": "^0.26.0"
    },
    "dependencies": {
        "@fontsource/roboto": "^5.0.8",
        "@hookform/resolvers": "^3.3.1",
        "@radix-ui/react-accordion": "^1.1.2",
        "@radix-ui/react-alert-dialog": "^1.0.5",
        "@radix-ui/react-aspect-ratio": "^1.0.3",
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-checkbox": "^1.0.4",
        "@radix-ui/react-collapsible": "^1.0.3",
        "@radix-ui/react-context-menu": "^2.1.5",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-hover-card": "^1.0.7",
        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-menubar": "^1.0.4",
        "@radix-ui/react-navigation-menu": "^1.1.4",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-progress": "^1.0.3",
        "@radix-ui/react-radio-group": "^1.1.3",
        "@radix-ui/react-scroll-area": "^1.0.5",
        "@radix-ui/react-select": "^2.0.0",
